pkgname=linux-nss
pkgver=3.12.4
pkgrel=1
pkgdesc='linux nss'
url='http://www.archlinux.org/'
license=('GPL')
arch=('any')
depends=('linux-base')
options=(!strip)
source=(
http://arm.konnichi.com/2009/12/01/extra/os/i686/nss-3.12.4-2-i686.pkg.tar.gz
)
md5sums=('bc30e5925b761d749c8a1908afdee2a9')
package() {
	install -dm755 "${pkgdir}/compat/linux"
	cp -Ra "${srcdir}/usr" "${pkgdir}/compat/linux"
	find ${pkgdir}/compat/linux/{bin,sbin,usr/bin,usr/sbin} -type f -print0 | xargs brandelf -t Linux
}
