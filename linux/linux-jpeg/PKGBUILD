pkgname=linux-jpeg
pkgver=2.18.3
pkgrel=1
pkgdesc='linux jpeg package'
url='http://www.archlinux.org/'
license=('GPL')
arch=('any')
options=(!strip)
source=(http://arm.konnichi.com/2009/12/01/extra/os/i686/libjpeg-7-1-i686.pkg.tar.gz)
md5sums=('9eef50c53c7a94c71216a63ead783eb4')

package() {
	install -dm755 "${pkgdir}/compat/linux"
	cp -Ra "${srcdir}/usr" "${pkgdir}/compat/linux"
}
