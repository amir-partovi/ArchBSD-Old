pkgrel=1
pkgdesc='linux hicolor-icon-theme'
url='http://www.archlinux.org/'
license=('GPL')
arch=('any')
depends=('linux-base')
options=(!strip)
source=(
	http://arm.konnichi.com/2009/12/01/extra/os/i686/hicolor-icon-theme-0.11-1-any.pkg.tar.gz 
) 
