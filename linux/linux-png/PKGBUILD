pkgname=linux-png
pkgver=1.2.40
pkgrel=1
pkgdesc='linux png package'
url='http://www.archlinux.org/'
license=('GPL')
arch=('any')
options=(!strip)
source=(http://arm.konnichi.com/2009/12/01/extra/os/i686/libpng-1.2.40-1-i686.pkg.tar.gz)
md5sums=('fb912bc28e5feaff6feeca9ac6c2cd36')

package() {
	install -dm755 "${pkgdir}/compat/linux"
	cp -Ra "${srcdir}/usr" "${pkgdir}/compat/linux"
}
