pkgname=linux-nspr
pkgver=4.8.9
pkgrel=1
pkgdesc='linux nspr'
url='http://www.archlinux.org/'
license=('GPL')
arch=('any')
depends=('linux-base')
options=(!strip)
source=(http://arm.konnichi.com/2011/09/05/extra/os/i686/nspr-4.8.9-1-i686.pkg.tar.xz)
md5sums=('f699d6790ac97d8b7f2d6f1887a477eb')

package() {
	install -dm755 "${pkgdir}/compat/linux"
	cp -Ra "${srcdir}/usr" "${pkgdir}/compat/linux"

    find ${srcdir} \
         -type f -print0 | xargs -0 file \
        | grep ELF | cut -d : -f 1 \
        | xargs brandelf -t Linux

}
