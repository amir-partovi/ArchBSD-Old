pkgname=linux-tiff
pkgver=3.9.5
pkgrel=1
pkgdesc='linux tiff'
url='http://www.archlinux.org/'
license=('GPL')
arch=('any')
depends=('linux-base' 'linux-jpeg')
options=(!strip)
source=(http://arm.konnichi.com/2011/09/05/extra/os/i686/libtiff-3.9.5-1-i686.pkg.tar.xz)
md5sums=('92af2e8e7fa03a4226d5013e1dc8c08b')

package() {
	install -dm755 "${pkgdir}/compat/linux"
	cp -Ra "${srcdir}/usr" "${pkgdir}/compat/linux"

        msg "Branding elf files"
        find ${srcdir} \
           -type f -print0 | xargs -0 file \
          | grep ELF | cut -d : -f 1 \
          | xargs brandelf -t linux
}
