pkgname=linux-atk
pkgver=2.18.3
pkgrel=1
pkgdesc='linux atk package'
url='http://www.archlinux.org/'
license=('GPL')
arch=('any')
options=(!strip)
depends=('linux-base')
source=(http://arm.konnichi.com/2009/12/01/extra/os/i686/atk-1.28.0-1-i686.pkg.tar.gz)
md5sums=('e40d94b243af714c39651d1e6523a384')

package() {
	install -dm755 "${pkgdir}/compat/linux"
	cp -Ra "${srcdir}/usr" "${pkgdir}/compat/linux"
}
