pkgname=linux-cairo
pkgver=1.8.8
pkgrel=1
pkgdesc='linux cairo'
url='http://www.archlinux.org/'
license=('GPL')
arch=('any')
depends=('linux-base' 'linux-expat' 'linux-fontconfig' 'linux-png' 
'linux-xorg-libs')
options=(!strip)
source=(
	http://arm.konnichi.com/2009/12/01/extra/os/i686/cairo-1.8.8-1-i686.pkg.tar.gz
)
md5sums=('5b4bb294bcbcf92315e4f67e75f9c9a9')

package() {
	install -dm755 "${pkgdir}/compat/linux"
	cp -Ra "${srcdir}/usr" "${pkgdir}/compat/linux"
}
