pkgname=linux-datrie
pkgver=0.2.4
pkgrel=1
pkgdesc='linux datrie'
url='http://www.archlinux.org/'
license=('GPL')
arch=('any')
depends=('linux-base')
options=(!strip)
source=(http://arm.konnichi.com/2011/09/05/extra/os/i686/libdatrie-0.2.4-1-i686.pkg.tar.xz)
md5sums=('78ffa7ecb47b4be8f6f639ae2ea87bc5')
package() {
	install -dm755 "${pkgdir}/compat/linux"
	cp -Ra "${srcdir}/usr" "${pkgdir}/compat/linux"

	msg "Branding elf files"
        find ${srcdir} \
           -type f -print0 | xargs -0 file \
          | grep ELF | cut -d : -f 1 \
          | xargs brandelf -t linux
}
