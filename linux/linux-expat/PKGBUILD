pkgname=linux-expat
pkgver=2.0.1
pkgrel=1
pkgdesc='linux expat package'
url='http://www.archlinux.org/'
license=('GPL')
arch=('any')
depends=('linux-base')
options=(!strip)
source=(http://arm.konnichi.com/2009/12/01/core/os/i686/expat-2.0.1-2-i686.pkg.tar.gz)
md5sums=('55f34b3b125cfde8a7924c96a79a258b')

package() {
	install -dm755 "${pkgdir}/compat/linux"
	cp -Ra "${srcdir}/usr" "${pkgdir}/compat/linux"
}
