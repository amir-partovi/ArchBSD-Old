post_install() {
    usr/bin/gdk-pixbuf-query-loaders > /dev/null 2>&1 && usr/bin/gdk-pixbuf-query-loaders > usr/lib/gdk-pixbuf-2.0/2.10.0/loaders.cache 2>/dev/null || /usr/bin/true
    /sbin/ldconfig -m /usr/lib
}

post_remove() {
    rm /usr/lib/gdk-pixbuf-2.0/2.10.0/loaders.cache
    /sbin/ldconfig -R
}
