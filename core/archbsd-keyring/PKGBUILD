pkgname=archbsd-keyring
pkgver=20121213
pkgrel=2
pkgdesc='Arch BSD PGP keyring'
arch=('any')
url='https://irc.unixhub.net/archbsd-keyring/'
license=('GPL')
install="${pkgname}.install"
source=("ftp://cpanel.archbsd.net/${pkgname}-${pkgver}.tar.gz"
	'archbsd-keyring.install')

md5sums=('54f329a7309cf995fe4a2c2cc85b0f82'
         '46712e950950fefa107bb5d9dee28f04')

package() {
	cd "${srcdir}/${pkgname}-${pkgver}"
	make PREFIX=/usr DESTDIR=${pkgdir} install
}

